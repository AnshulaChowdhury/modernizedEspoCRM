# EspoCRM Docker Configuration
# Copy this file to .env and adjust values as needed

# Application ports
ESPOCRM_PORT=8080
WEBSOCKET_PORT=8081
POSTGRES_PORT=5432

# PostgreSQL database credentials
POSTGRES_DB=espocrm
POSTGRES_USER=espocrm
POSTGRES_PASSWORD=espocrm

# EspoCRM admin credentials (used during initial setup)
ESPOCRM_ADMIN_USERNAME=admin
ESPOCRM_ADMIN_PASSWORD=admin
